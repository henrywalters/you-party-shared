  "bugs": {
    "url": "https://github.com/henrywalters/you-party-shared/issues"
  },
  "homepage": "https://github.com/henrywalters/you-party-shared#readme",
  "devDependencies": {
    "prettier": "^1.17.1",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  }
}
